<h1>Users</h1>

<%= render 'users/info', user: current_user %>

<%= render 'books/newbook', book: @book %>

 <table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
 <% @users.each do |user| %>
 <tr>
 <td>
  <%= image_tag user.get_profile_image(100,100) %>
 </td>

 <td><%= user.name %></td>

 <td><%= link_to "Show", user_path(current_user.id) %></td>
 <% end %>
 </tbody>
 </table>


